    "pivot_sibsp = pd.pivot_table(train, index='SibSp', values='Survived', aggfunc=np.mean)\n",
    "pivot_sibsp.plot.bar(rot=0)\n",
    "plt.title('Proportion of survivals\\nper number of siblings/spouses', y=1.07)\n",
